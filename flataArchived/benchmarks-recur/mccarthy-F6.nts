nts mccarthy;


f {
  in x : int;
  out y : int;
  initial s1;
  final sf;

  s1 -> sf { x > 100 && y' = x-10 && havoc(y) }
  s1 -> s2 { x <= 100 && y' = x+51 && havoc(y) }
  s2 -> s3 { y'=f(y) && havoc(y) }
  s3 -> s4 { y'=f(y) && havoc(y) }
  s4 -> s5 { y'=f(y) && havoc(y) }
  s5 -> s6 { y'=f(y) && havoc(y) }
  s6 -> s7 { y'=f(y) && havoc(y) }
  s7 -> sf { y'=f(y) && havoc(y) }
}

